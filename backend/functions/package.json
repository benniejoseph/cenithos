    "firebase-functions": "^6.3.2",
    "twilio": "^5.0.4"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.12",
    "node-fetch": "^2.7.0",
    "typescript": "^5.4.5",
    "ts-node": "^10.9.2",
    "eslint-plugin-import": "^2.29.1",
    "firebase-functions-test": "^3.2.0"
  }
}
